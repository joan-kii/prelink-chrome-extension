{
  "name": "PreLink",
  "description": "Previsualize and get info about a link clicking on it",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "activeTab"
  ],
  "action": {
    "default_icon": {
      "16": "/assets/prelink16.png",
      "32": "/assets/prelink32.png",
      "48": "/assets/prelink48.png",
      "128": "/assets/prelink128.png"
    }
  },
  "icons": {
    "16": "/assets/prelink16.png",
    "32": "/assets/prelink32.png",
    "48": "/assets/prelink48.png",
    "128": "/assets/prelink128.png"
  }
}
